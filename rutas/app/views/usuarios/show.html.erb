<p id="notice"><%= notice %></p>

<p>
  <strong>Nombre:</strong>
  <%= @usuario.nombre %>
</p>

<p>
  <strong>Apellido:</strong>
  <%= @usuario.apellido %>
</p>

<p>
  <strong>Edad:</strong>
  <%= @usuario.edad %>
</p>

<p>
  <strong>Email:</strong>
  <%= @usuario.email %>
</p>

<p>
  <strong>Password digest:</strong>
  <%= @usuario.password_digest %>
</p>

<p>
  <strong>Rol:</strong>
  <%= @usuario.rol_id %>
  <% @rols.each do |rol| %>
    <% if rol.id == @usuario.rol_id %>
        <%= rol.nombre %>
    <% end %>
  <% end %>
</p>

  

<%= link_to 'Edit', edit_usuario_path(@usuario) %> |
<%= link_to 'Back', usuarios_path %>
